{
  "name": "demotivator",
  "private": true,
  "version": "12.0.0-beta-2",
  "type": "module",
  "scripts": {
    "dev": "vite serve",
    "build": "vite build",
    "ssrBuild": "vite ssrBuild",
    "lint": "eslint ./src/* --fix",
    "buildTS": "tsc --project tsconfig.json",
    "typeCheck": "tsc --project tsconfig.json --noEmit",
    "supportedBrowsers": "(echo export default && browserslist-useragent-regexp --allowHigherVersions) > src/utils/supportedBrowsers.ts",
    "preview": "vite preview",
    "push": "firebase deploy",
    "ionic:build": "npm run build",
    "capBuild": "npx cap sync && npx cap sync electron && cd ./electron && npm run build && npm run electron:start-live"
  },
  "devDependencies": {
    "@capacitor/cli": "^4.6.3",
    "@sveltejs/vite-plugin-svelte": "^2.0.2",
    "autoprefixer": "^10.4.13",
    "browserslist-useragent-regexp": "^4.0.0",
    "eslint": "^8.34.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "postcss": "^8.4.21",
    "sass": "^1.58.0",
    "svelte": "^3.55.1",
    "svelte-preprocess": "^5.0.1",
    "tailwindcss": "^3.2.6",
    "vite": "^4.1.1"
  },
  "dependencies": {
    "@capacitor-community/electron": "^4.1.2",
    "@capacitor/android": "^4.6.3",
    "@capacitor/core": "^4.6.3",
    "@capacitor/dialog": "^4.1.0",
    "@capacitor/haptics": "^4.1.0",
    "@capacitor/ios": "^4.6.3",
    "@capacitor/splash-screen": "^4.1.4",
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.3",
    "canvas-confetti": "^1.6.0",
    "cordova-res": "^0.15.4",
    "demotivator": "^11.1.0",
    "firebase": "^9.17.1",
    "firebase-tools": "^11.22.0",
    "serve": "^14.2.0",
    "svelte-loading-spinners": "^0.1.7",
    "terser": "^5.16.3",
    "twemoji": "^14.0.2",
    "typescript": "^4.9.5",
    "uadetect": "^5.0.0"
  }
}
