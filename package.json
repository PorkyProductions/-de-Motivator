{
  "name": "demotivator",
  "private": true,
  "version": "10.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite serve",
    "build": "vite build",
    "ssrBuild": "vite ssrBuild",
    "lint": "eslint ./src/* --fix",
    "buildTS": "tsc --project tsconfig.json",
    "typeCheck": "tsc --project tsconfig.json --noEmit",
    "supportedBrowsers": "(echo export default && browserslist-useragent-regexp --allowHigherVersions) > supportedBrowsers.js",
    "preview": "vite preview",
    "push": "firebase deploy",
    "ionic:build": "npm run build",
    "capBuild": "npx cap sync && npx cap sync electron && cd ./electron && npm run build && npm run electron:start-live"
  },
  "devDependencies": {
    "@capacitor/cli": "^4.1.0",
    "@sveltejs/vite-plugin-svelte": "^1.0.8",
    "autoprefixer": "^10.4.11",
    "browserslist-useragent-regexp": "^4.0.0",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "postcss": "^8.4.16",
    "sass": "^1.55.0",
    "svelte": "^3.49.0",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.8",
    "vite": "^3.2.4"
  },
  "dependencies": {
    "@capacitor-community/electron": "^4.1.2",
    "@capacitor/android": "^4.2.0",
    "@capacitor/core": "^4.0.0",
    "@capacitor/dialog": "^4.1.0",
    "@capacitor/haptics": "^4.1.0",
    "@capacitor/ios": "^4.2.0",
    "@capacitor/splash-screen": "^4.1.2",
    "@vitejs/plugin-legacy": "^2.3.1",
    "bootstrap": "^5.2.2",
    "bootstrap-icons": "^1.8.3",
    "canvas-confetti": "^1.6.0",
    "cordova-res": "^0.15.4",
    "demotivator": "^11.1.0",
    "firebase": "^9.10.0",
    "firebase-tools": "^11.10.0",
    "serve": "^14.1.2",
    "svelte-loading-spinners": "^0.1.7",
    "terser": "^5.15.1",
    "twemoji": "^14.0.2",
    "typescript": "^4.7.4",
    "uadetect": "^5.0.0"
  }
}
